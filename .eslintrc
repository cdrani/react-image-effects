{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "impliedStrict": true
    }
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "extends": ["prettier", "prettier/react"],
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "react/jsx-uses-react": 1,
  }
}
