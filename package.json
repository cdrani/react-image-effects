{
  "name": "react-image-effects",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "description": "react image effects",
  "keywords": [
    "react",
    "reactjs",
    "image",
    "react-image",
    "image-effects"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdrani/react-image-effects.git"
  },
  "scripts": {
    "start": "rollup -c -w",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "rollup -c",
    "predeploy": "cd example && yarn && yarn build",
    "deploy": "gh-pages example/build"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{js,md}": [
      "prettier-eslint --list-different",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": ">= 16.x.x"
  },
  "author": "cdrani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdrani/react-image-effects/issues"
  },
  "homepage": "https://github.com/cdrani/react-image-effects",
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.15.3",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "rollup": "1.6.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-url": "2.2.0",
    "styled-components": "4.1.3"
  },
  "files": [
    "dist"
  ]
