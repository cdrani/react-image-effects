{
  "name": "react-image-effects",
  "version": "0.1.0",
  "main": "index.js",
  "description": "react image effects",
  "scripts": {
    "prepublishOnly": "yarn release",
    "release": "yarn build:dev && yarn build:prod",
    "build:prod": "NODE_ENV=production rollup -c",
    "build:dev": "NODE_ENV=development rollup -c",
    "start": "NODE_ENV=development rollup -c -w"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{js,md}": [
      "prettier-eslint --list-different",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "react",
    "reactjs",
    "image",
    "react-image",
    "effects"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdrani/react-image-effects.git"
  },
  "peerDependencies": {
    "react": ">= 16.x.x"
  },
  "author": "cdrani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdrani/react-image-effects/issues"
  },
  "homepage": "https://github.com/cdrani/react-image-effects",
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.15.3",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "rollup": "1.6.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-replace": "2.1.0",
    "styled-components": "4.1.3"
  }
}
